{
    "name": "react-huge-icons",
    "license": "MIT",
    "version": "1.3.0",
    "description": "A Huge Icons icon pack made by Zero-Icon team. Developing ...",
    "keywords": [
        "react",
        "react-icons",
        "icons",
        "svg",
        "icon-pack",
        "react-huge-icons",
        "Zero-Icon"
    ],
    "types": "react-huge-icons/index.d.ts",
    "scripts": {
        "build": "npm run rimraf && npm run build:outline && npm run build:solid && npm run build:bulk",
        "rimraf": "rimraf ./react-huge-icons-develop/_svg/bulk/optimized && rimraf ./react-huge-icons-develop/_svg/outline/optimized && rimraf ./react-huge-icons-develop/_svg/solid/optimized && rimraf ./react-huge-icons-develop/components/bulk && rimraf ./react-huge-icons-develop/components/outline && rimraf ./react-huge-icons-develop/components/solid && rimraf ./react-huge-icons/bulk/ && rimraf ./react-huge-icons/outline/ && rimraf ./react-huge-icons/solid/",
        "build:outline": "svgo -f react-huge-icons-develop/_svg/outline -o react-huge-icons-develop/_svg/outline/optimized && svgr -d react-huge-icons-develop/components/outline react-huge-icons-develop/_svg/outline/optimized",
        "build:solid": "svgo -f react-huge-icons-develop/_svg/solid -o react-huge-icons-develop/_svg/solid/optimized && svgr -d react-huge-icons-develop/components/solid react-huge-icons-develop/_svg/solid/optimized",
        "build:bulk": "svgo -f react-huge-icons-develop/_svg/bulk -o react-huge-icons-develop/_svg/bulk/optimized && svgr -d react-huge-icons-develop/components/bulk react-huge-icons-develop/_svg/bulk/optimized",
        "rollup": "rollup -c && npm run rimraf:types",
        "rimraf:types": "rimraf ./react-huge-icons/bulk/types && rimraf ./react-huge-icons/outline/types && rimraf ./react-huge-icons/solid/types",
        "copy:types": "cp -r ./react-huge-icons/bulk/types/bulk ./types-react-huge-icons/bulk && cp -r ./react-huge-icons/outline/types/outline ./types-react-huge-icons/outline && cp -r ./react-huge-icons/solid/types/solid ./types-react-huge-icons/solid"
    },
    "dependencies": {
        "@babel/runtime": "^7.21.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^5.0.3"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.21.3",
        "@babel/plugin-transform-runtime": "^7.21.0",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-typescript": "^11.0.0",
        "@svgr/cli": "^7.0.0",
        "@types/react": "^18.0.32",
        "babel-loader": "^9.1.2",
        "babel-preset-minify": "^0.5.2",
        "prettier": "2.8.7",
        "rimraf": "^4.4.1",
        "rollup": "^2.79.1",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-dts": "^5.3.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "tslib": "^2.5.0"
    }
}
